public class PrimeNumber {
	public static void main (String [] args)
	{
		int n;
		int start = 1;
		int num = 3;
		System.out.println("First 100 prime number are");
		System.out.println(2);
		for(int i=2; i<=100;)
		{
			for(int j=2; j<=Math.sqrt(num);j++)
			{
				if(num%j == 0)
				{
					start =0;
					break;
					
				}
			}
			if (start != 0)
			{
				System.out.println(num);
				i++;
			}
			start = 1;
			num++;
		}
		
	}

}
